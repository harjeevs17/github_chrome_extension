{
    "manifest_version":2,
    "name":"GitBot",
    "description" : "GitBot is a chrome extension that displays the total number of PRs under your name in a convenient format.",
    "version" : "1.0.0",
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "permissions":[
        "storage"
    ],
    "content_security_policy": "script-src-elem 'self' * 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-hashes' 'unsafe-inline'"
    


}